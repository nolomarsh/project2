<!DOCTYPE html>
<html lang="en" dir="ltr">
    <head>
        <%-include('../partials/head.ejs')%>
        <meta charset="utf-8">
        <title></title>
    </head>
    <body>
        <h1>All Pokemon</h1>
        <% for (let pokemon of allPokemon) { %>
            <div class="card">
                <a href="/pokemon/<%=pokemon._id%>">
                    <img class="card-img-top" src="<%=pokemon.img%>" alt="<%=pokemon.name%>">
                </a>
                <h3 class="card-title"><%=pokemon.name%></h3>
            </div>
        <% } %>
    </body>
</html>
